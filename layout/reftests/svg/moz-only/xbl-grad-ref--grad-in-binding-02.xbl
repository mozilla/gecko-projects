<bindings xmlns="http://www.mozilla.org/xbl">
  <binding id="lime">
    <content>
      <children/>
      <g xmlns="http://www.w3.org/2000/svg">
        <linearGradient id="grad" gradientUnits="userSpaceOnUse">
          <stop stop-color="lime"/>
        </linearGradient>
        <rect width="100%" height="100%" fill="url(#grad)"/>
      </g>
    </content>
  </binding>
</bindings>
