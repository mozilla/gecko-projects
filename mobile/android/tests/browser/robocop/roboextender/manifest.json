{
  "manifest_version": 2,
  "name": "Robocop Extender",
  "version": "2.0",

  "applications": {
    "gecko": {
      "id": "roboextender@mozilla.org"
    }
  },
  "permissions": [],
  "experiment_apis": {
    "roboextender": {
      "schema": "schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "api.js",
        "events": ["startup"]
      }
    }
  }
}
